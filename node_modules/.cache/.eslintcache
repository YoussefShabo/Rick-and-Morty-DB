)\n  setHideC(false)\n  setHideL(false)\n  setHideImage(false)\n}\n\n\nconst showImage = () => {\n  setHideE(false)\n  setHideC(false)\n  setHideL(false)\n  setHideImage(true)\n}\n\n\n\n\n\n\n  useEffect(() => {\n    getCharacters()\n }, [])\n  useEffect(() => {\n    getLocations()\n }, [])\n  useEffect(() => {\n    getEpisodes()\n }, [])\n\n  return (\n\n    <div className=\"App\" id=\"app\">\n\n      <div id=\"buttons\">\n        <h4 id=\"button\" onClick={showCharacters}>Characters</h4>\n        <h4 id=\"button\" onClick={showLocations}>Locations</h4>\n        <img src=\"pngegg.png\" alt=\"\" width=\"300\"/>\n        <h4 id=\"button\" onClick={showEpisodes}>Episodes</h4>\n        <h4 id=\"button\" onClick={home}>Home</h4>\n      </div>\n      <img id=\"giant\" src=\"giant.png\"/>\n      <img id=\"planet\" src=\"planet.png\"/>\n      <img id=\"ship\" src=\"ship.png\"/>\n\n      <div class=\"displayC\">\n        {characters.map((character) => {\n          return(\n            <div id=\"cards\"> \n              {hideC \n              ? <Character character={character}/>\n              : \"\" }   \n            </div>\n          )\n        })}\n      </div>\n      <div class=\"displayL\">\n        {locations.map((location) => {\n          return(\n            <div> \n              {hideL \n              ? <Location location={location}/>\n              : \"\" }   \n            </div>\n          )\n        })}\n      </div>\n      <div class=\"displayE\">\n        {episodes.map((episode) => {\n          return(\n            <div> \n              {hideE \n              ? <Episode episode={episode}/>\n              : \"\" }   \n            </div>\n          )\n        })}\n      </div>\n      <div class=\"displayImage\"> \n              {hideImage \n              ? <img id=\"ram\" src=\"ram.png\"/>\n              : \"\" }   \n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/youssefshabo/dev/Ollivanders/homework/24_rick_and_morty-YoussefShabo/ram-app/src